<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bubbel"
        tools:targetApi="31">
        <activity
            android:name=".CustomWidgets.ui.theme.DynamicWidgetSizes"
            android:exported="false"
            android:label="@string/title_activity_dynamic_widget_sizes"
            android:theme="@style/Theme.Bubbel" />
        <activity
            android:name=".Screens.LoginView"
            android:exported="false"
            android:label="@string/title_activity_login_view"
            android:theme="@style/Theme.Bubbel" />
        <activity
            android:name=".Screens.ScreenView"
            android:exported="false"
            android:label="@string/title_activity_screen_view"
            android:theme="@style/Theme.Bubbel" />
        <activity
            android:name=".Screens.LoginScreen"
            android:exported="false"
            android:label="@string/title_activity_login_screen"
            android:theme="@style/Theme.Bubbel" />
        <activity
            android:name=".CustomWidgets.ui.theme.DynamicPosition"
            android:exported="false"
            android:label="@string/title_activity_dynamic_position"
            android:theme="@style/Theme.Bubbel" />
        <activity
            android:name=".CustomWidgets.DynamicPositioning.kt"
            android:exported="false"
            android:label="@string/title_activity_dynamic_positioning.kt"
            android:theme="@style/Theme.Bubbel" />
        <activity
            android:name=".Screens.SignUpScreen"
            android:exported="false"
            android:label="@string/title_activity_sign_up_screen"
            android:theme="@style/Theme.Bubbel" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Bubbel">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>